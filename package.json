{
  "name": "nix-eval-js",
  "version": "0.0.1",
  "description": "run nix code in javascript",
  "main": "src/nix-eval.js",
  "type": "module",
  "bin": {
    "nix-eval": "./src/nix-eval-cli.js"
  },
  "scripts": {
    "test": "ava",
    "__test": "run-s test:unit",
    "__test:unit": "ava",
    "test:perf": "bipbip --compare doc/benchmarks.json test/benchmarks.js",
    "test:perf:save": "bipbip --compare doc/benchmarks.json --save doc/benchmarks.json test/benchmarks.js"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "verbose": true
  },
  "keywords": [
    "nix",
    "nixos",
    "nixpkgs",
    "eval",
    "evaluator",
    "interpreter",
    "incremental-interpreter",
    "incremental-evaluator",
    "incremental-evaluation",
    "incremental-compiler",
    "incremental-compilation",
    "live-coding",
    "lezer-parser",
    "vanillajs",
    "toy-interpreter"
  ],
  "author": "Milan Hauth",
  "license": "MIT",
  "dependencies": {
    "@lezer/lr": "^1.2.3"
  },
  "devDependencies": {
    "@lezer/common": "^1.0.1",
    "ava": "^4.3.3"
  },
  "devDependencies__benchmark": {
    "npm-run-all": "^4.1.5",
    "bipbip": "github:milahu/bipbip"
  }
}
